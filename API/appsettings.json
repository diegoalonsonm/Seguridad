{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Seguridad": "Server=tcp:sqlserverdanm.database.windows.net,1433;Initial Catalog=seguridad;Persist Security Info=False;User ID=${DB_USER};Password=${DB_PASS};MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"
